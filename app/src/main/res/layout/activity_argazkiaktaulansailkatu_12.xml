<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Argazkiaktaulansailkatu_12">

    <ImageButton
        android:id="@+id/gizaetxea"
        android:layout_width="160dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gizaetxea" />

    <ImageButton
        android:id="@+id/musika"
        android:layout_width="160dp"
        android:layout_height="90dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/musika" />

    <ImageButton
        android:id="@+id/sutea"
        android:layout_width="160dp"
        android:layout_height="90dp"
        android:layout_marginStart="124dp"
        android:layout_marginBottom="36dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/respuesta2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/sutea" />

    <ImageButton
        android:id="@+id/zinema"
        android:layout_width="160dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toStartOf="@+id/musika"
        app:layout_constraintStart_toEndOf="@+id/gizaetxea"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/zinema" />

    <ImageButton
        android:id="@+id/otoitza"
        android:layout_width="160dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="36dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/respuesta4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toEndOf="@+id/sutea"
        app:srcCompat="@drawable/otoitza" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/josefin_slab_bold"
        android:text="@string/textoorden"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/respuesta3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/btnPreviousGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        android:clickable="true"
        app:backgroundTint="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/imgprev" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/btnNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:clickable="true"
        app:backgroundTint="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/imgnext" />

    <ImageView
        android:id="@+id/respuesta1"
        android:layout_width="108dp"
        android:layout_height="69dp"
        android:layout_marginBottom="40dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/respuesta2"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/respuesta2"
        android:layout_width="108dp"
        android:layout_height="69dp"
        android:layout_marginBottom="40dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/respuesta3"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/respuesta3"
        android:layout_width="108dp"
        android:layout_height="69dp"
        android:layout_marginBottom="40dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/respuesta4"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/respuesta4"
        android:layout_width="108dp"
        android:layout_height="69dp"
        android:layout_marginBottom="40dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/respuesta5"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/respuesta5"
        android:layout_width="108dp"
        android:layout_height="69dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="40dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:srcCompat="@tools:sample/avatars[3]" />

</android.support.constraint.ConstraintLayout>